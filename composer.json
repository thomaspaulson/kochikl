{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.6.0",
		"silverstripe/framework": "3.6.0",
		"silverstripe/reports": "3.6.0",
		"silverstripe/siteconfig": "3.6.0",
		"silverstripe-themes/simple": "3.1.*",
		"silverstripe/blog": "2.*@dev",
		"unclecheese/betterbuttons": "1.3.*@dev",
		"heyday/silverstripe-menumanager": "2.2.2",
		"colymba/gridfield-bulk-editing-tools": "2.1.10",
		"undefinedoffset/sortablegridfield": "1.0.*@dev"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.6.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
